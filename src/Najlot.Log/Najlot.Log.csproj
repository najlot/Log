<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Najlot</Authors>
    <Company>-</Company>
    <PackageProjectUrl>https://github.com/najlot/Log</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/najlot/Log/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/najlot/Log</RepositoryUrl>
    <Product>Najlot.Log</Product>
    <Copyright>Najlot</Copyright>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageTags>najlot;log;logging;logger;najlot.log</PackageTags>
    <PackageIconUrl>https://github.com/najlot/Log/blob/master/images/icon.png?raw=true</PackageIconUrl>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Description>Simple and yet powerful logging library with performance and extensibility in mind.</Description>
    <Version>2.1.1</Version>
  </PropertyGroup>

</Project>
